Rhymes = (word) => {\n    var rhyme_list = rhyme(word);\n    var new_rhymes = [];\n\n    if (rhyme_list.length < 1) {\n      setRhymes([\"No rhymes in database D:\"])\n      return;\n    }\n    else if (rhyme_list.length < 3) {\n      rhyme_list.forEach(item => {\n        new_rhymes.unshift(item['word'].toUpperCase())\n      })\n    }\n    else {\n      for(var i = 0; i < 3; i++) {\n        var randInt = randomInt(1, rhyme_list.length)\n        var lastWord = \"\";\n        var rh = rhyme_list[randInt]['word'].toUpperCase()\n\n        if (new_rhymes.includes(rh) || lastWord.charAt(0) === rh.charAt(0) || rh === word) {\n          i -= 1;\n          lastWord = \"\";\n        } else {\n          new_rhymes.unshift(rh)\n          lastWord = rh\n        }\n      }\n    }\n\n    setRhymes(new_rhymes);\n  }\n\n  return (\n    <HashRouter basename='/'>\n      <div className=\"App\">\n        <Header className = \"head\"/>\n        <Route path = '/' exact render = {(props) => (\n          <>\n            <Button class = \"btn\" divName = 'btndiv' text = \"Let's Go!\" onClick = {generateWord}/>\n\n            <Word word = {currentWord} class = \"wrd\"/>\n            <Rhymes rhymes = {rhymes} class = \"subwrds\"/>\n\n            <Footer />\n          </>\n        )} />\n\n        <Route path = '/about' exact component = {About} />\n      </div>\n    </HashRouter>\n  );\n}\n\nexport default App;\n","C:\\Users\\deonm\\Documents\\UNI\\React\\random-word-gen\\src\\reportWebVitals.js",[],"C:\\Users\\deonm\\Documents\\UNI\\React\\random-word-gen\\src\\components\\Button.js",[],"C:\\Users\\deonm\\Documents\\UNI\\React\\random-word-gen\\src\\components\\Word.js",[],"C:\\Users\\deonm\\Documents\\UNI\\React\\random-word-gen\\src\\components\\Header.js",[],"C:\\Users\\deonm\\Documents\\UNI\\React\\random-word-gen\\src\\components\\Rhymes.js",[],"C:\\Users\\deonm\\Documents\\UNI\\React\\random-word-gen\\src\\components\\Footer.js",[],"C:\\Users\\deonm\\Documents\\UNI\\React\\random-word-g